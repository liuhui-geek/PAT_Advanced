#include<iostream>
#include<algorithm>
#include<math.h>
using namespace std;
//1046
int main(){
	int n,num,pre[100002],query,a,b,total = 0;
	scanf("%d", &n);
	pre[0] = 0;
	for (int i = 1; i <= n; i++) {
		scanf("%d", &num);
		total += num;
		pre[i] = pre[i - 1] + num;
	}
	scanf("%d", &query);
	for (int i = 0; i < query; i++) {
		int path1 = 0,path2 = 0,path = 0;
		scanf("%d%d", &a, &b);
		if (a > b)
			swap(a, b);
		path1 = pre[b - 1] - pre[a - 1];
		path2 = total - path1;
		path = min(path1, path2);
		printf("%d\n", path);
	}
	return 0;
}
