#include<iostream>
#include<vector>
#include<queue>
using namespace std;

int main() {
	vector<int> seq;
	queue<int> test;
	int d = 0, n = 0;
	int sum = 0,currentnum = 0,topnum = 0;
	cin >> d >> n;
	if (n == 1) {
		cout << d;
	}
	else if(n==2){
		cout << d<<1;
	}
	else {
		n = n - 2;
		test.push(d);
		test.push(1);
		while (true) {
			while (true){
				currentnum = test.front();
				sum++;
				test.pop();
				if (test.empty()) {
					seq.push_back(currentnum);
					seq.push_back(sum);
					sum = 0;
					break;
				}
				topnum = test.front();
				if (topnum != currentnum) {
					seq.push_back(currentnum);
					seq.push_back(sum);
					sum = 0;
				}
			}
			n--;
			if (n == 0) {
				break;
			}
			vector<int>::iterator midd = seq.begin();
			while (midd!=seq.end()) {
				test.push(*midd);
				midd++;
			}
			seq.clear();
		}
		vector<int>::iterator midd2 = seq.begin();
		while (midd2 != seq.end()) {
			cout<<*midd2;
			midd2++;
		}
	}
	return 0;
}