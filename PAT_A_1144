//使用map，填入数存在标记为true
#include<iostream>
#include<map>
using namespace std;
#define INT_MAX 2147483647;
int main() {
	map<int, bool> amap;
	int n = 0;
	int num = 0;
	cin >> n;
	int max = INT_MAX;
	for (int i = 0; i < n; i++) {
		cin >> num;
		if (num > 0);
			amap[num] = true;
	}
	for (int i = 1; i <= max; i++) {
		if (amap[i] == false) {
			cout << i;
			return 0;
		}
	}
	return 0;
}